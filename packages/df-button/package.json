{
  "name": "df-button",
  "version": "0.1.1",
  "description": "Custom Button component built with tailwindcss",
  "author": "denisfloyd",
  "homepage": "https://github.com/denisfloyd/my-turbo-components/blob/main/packages/df-button#readme",
  "bugs": {
    "url": "https://github.com/denisfloyd/my-turbo-components/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/denisfloyd/my-turbo-components.git",
    "directory": "/packages/df-button"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "license": "MIT",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/index.css"
  },
  "scripts": {
