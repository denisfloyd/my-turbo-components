    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -rf coverage"
  },
  "devDependencies": {
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "cssnano": "^6.0.1",
    "df-tsconfig": "workspace:*",
    "eslint-config-df": "workspace:*",
    "postcss": "^8.4.20",
    "tailwind-config": "workspace:*",
    "tsup": "^5.12.9",
    "typescript": "^4.5.3",
    "jest-config": "workspace:*"
  },
  "dependencies": {
    "tailwind-variants": "^0.1.13",
    "react": "^18.1.0"
  },
  "publishConfig": {
