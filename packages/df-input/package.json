{
  "name": "df-input",
  "version": "0.2.0",
  "description": "Custom Input component built with tailwindcss",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/denisfloyd/my-turbo-components.git",
    "directory": "/packages/df-input"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "license": "MIT",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/index.css"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .tsx,.ts --max-warnings=0",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "df-tsconfig": "workspace:*",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "cssnano": "^6.0.1",
    "eslint-config-df": "workspace:*",
    "postcss": "^8.4.20",
    "react": "^18.1.0",
    "tailwind-config": "workspace:*",
    "tsup": "^5.12.9",
    "typescript": "^4.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
